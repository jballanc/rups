(def tuple_struct
  (Struct. :x :y))

(def mk_tuple (lambda [x y] (.new tuple_struct x y)))

(defn mk_tuple [x y]
  (.new tuple_struct x y))

(defmacro defn [name args body]
  `(def ,name (lambda ,args ,body))

  (list 'def name (list 'lambda args body)))







